{
    "name": "VoiceChatStreamer",
    "description": "An Telegram Bot To Stream Videos and Audios in Voice Chat",
    "stack": "container",
    "keywords": [
      "telegram",
      "voicechat",
      "bot"
    ],
    "env": {
      "API_ID": {
        "description": "Your API ID from https://my.telegram.org",
        "required": true
      },
      "API_HASH": {
        "description": "Your API HASH from https://my.teleram.org",
        "required": true
      },
      "SESSION": {
        "description": "Get Pyrogram string session from https://replit.com/@AnjanaMadu/GenerateStringSession",
        "required": true
      },
      "TOKEN": {
        "description": "Your BOT TOKEN from https://t.me/BotFather",
        "required": true
      },
      "ADMINS": {
        "description": "Your telegram ID. If 2 or more, Seperate by space.",
        "required": true
      },
      "CHAT_ID": {
        "description": "Chat ID where need to bot work.",
        "required": true
      }
    }
  }
